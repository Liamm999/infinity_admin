    "dayjs": "^1.11.10",
    "env-cmd": "^10.1.0",
    "framer-motion": "^10.16.4",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "query-string": "^8.1.0",
